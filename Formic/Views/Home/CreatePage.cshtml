@model RecordSet

@{
    ViewData["Formic.Title"] = "Create a " + Model.EntityName;
}

@using (Html.BeginForm())
{
    <table>
    @foreach (var prop in Model.Properties.Where(p => !p.IsPrimaryKey))
    {
        <tr>
            <th>@prop.Description:</th>
            <td>@Html.EditorProperty(null, prop.Property)</td>
        </tr>
    }
        <tr>
            <td colspan="2">
                <input type="submit" value="Save" />
            </td>
        </tr>
    </table>

}

